{
  "name": "9001app2",
  "version": "1.0.0",
  "description": "Sistema de GestiÃ³n de Calidad ISO 9001",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "install-all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "auto-planner": "node scripts/run-auto-planner.js",
    "auto-planner:help": "node scripts/run-auto-planner.js --help",
    "auto-planner:dry-run": "node scripts/run-auto-planner.js --dry-run --verbose",
    "auto-planner:refactor": "node scripts/run-auto-planner.js --phase refactor",
    "auto-planner:typescript": "node scripts/run-auto-planner.js --phase typescript",
    "auto-planner:hallazgos": "node scripts/run-auto-planner.js --modules hallazgos",
    "auto-planner:acciones": "node scripts/run-auto-planner.js --modules acciones",
    "dashboard": "node scripts/auto-planner-dashboard.js",
    "dashboard:install": "npm install express socket.io open",
    "dashboard:setup": "node scripts/setup-dashboard.js",
    "create-portable": "node scripts/create-portable-agent.js"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "express": "^5.1.0",
    "open": "^10.2.0",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "concurrently": "^8.0.0"
  }
}
